{
  "workDir": ".",
  "dependencies": [],
  "prebuild": [
    {
      "name": "PSK bundles",
      "src": "",
      "actions": [
        {
          "type": "copy",
          "src": "../privatesky/psknode/bundles/pskruntime.js",
          "target": "code/constitution/pskruntime.js",
          "options": {
            "overwrite": true
          }
        },
        {
          "type": "copy",
          "src": "../privatesky/psknode/bundles/webshims.js",
          "target": "code/constitution/webshims.js",
          "options": {
            "overwrite": true
          }
        },
        {
          "type": "copy",
          "src": "../privatesky/psknode/bundles/iframeBoot.js",
          "target": "code/constitution/iframeBoot.js",
          "options": {
            "overwrite": true
          }
        },
        {
          "type": "copy",
          "src": "../privatesky/psknode/bundles/pskruntime.js",
          "target": "code/scripts/bundles/pskruntime.js",
          "options": {
            "overwrite": true
          }
        },
        {
          "type": "copy",
          "src": "../privatesky/psknode/bundles/webshims.js",
          "target": "code/scripts/bundles/webshims.js",
          "options": {
            "overwrite": true
          }
        },
        {
          "type": "copy",
          "src": "../privatesky/psknode/bundles/iframeBoot.js",
          "target": "code/scripts/bundles/iframeBoot.js",
          "options": {
            "overwrite": true
          }
        }
      ]
    },
    {
      "name": "Ionic dependency",
      "actions": [
        {
          "type": "copy",
          "src": "./node_modules/@ionic/core/dist/ionic",
          "target": "./code/ionic/dist"
        },
        {
          "type": "copy",
          "src": "./node_modules/@ionic/core/css",
          "target": "./code/ionic/css"
        }
      ]
    },
    {
      "name": "Shoelace dependency",
      "actions": [
        {
          "type": "copy",
          "src": "./node_modules/@shoelace-style/shoelace/dist",
          "target": "./code/shoelace/dist"
        }
      ]
    },
    {
      "name": "JSONEditor dependency",
      "actions": [
        {
          "type": "copy",
          "src": "./node_modules/jsoneditor/dist",
          "target": "./code/jsoneditor/dist"
        }
      ]
    }
  ],
  "postbuild": [
    {
      "name": "post build clean up",
      "actions": [
        {
          "type": "remove",
          "target": "./builds"
        }
      ]
    }
  ]
}
